# LiveKit credentials (from livekit.cloud)
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your-api-key
LIVEKIT_API_SECRET=your-api-secret

# Clawdbot Gateway
CLAWDBOT_GATEWAY_URL=http://localhost:3033
CLAWDBOT_GATEWAY_TOKEN=your-gateway-token
CLAWDBOT_SESSION_KEY=agent:voice  # Session key for continuity

# STT - Deepgram
DEEPGRAM_API_KEY=your-deepgram-key

# TTS - ElevenLabs
ELEVENLABS_API_KEY=your-elevenlabs-key
ELEVENLABS_VOICE_ID=your-voice-id  # e.g., "pNInz6obpgDQGcFmaJgB" (Adam)
